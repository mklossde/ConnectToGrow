<configuration>
    <statusListener class="ch.qos.logback.core.status.OnConsoleStatusListener"/>
    
 
    
		
	<appender class="ch.qos.logback.core.ConsoleAppender" name="STDOUT">
        <encoder>
            <pattern>%-5level %logger{36}.%M - %msg%n</pattern>
        </encoder>
    </appender>

        
    <root level="DEBUG">
       <appender-ref ref="STDOUT"/>
   	</root>
            
    

    
</configuration>
